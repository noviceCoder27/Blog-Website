{
    "version":2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        {
          "src": "/(.*)",
          "dest": "/",
          "methods":["GET","PUT","PATCH","POST","DELETE"],
          "headers": {
            "Access-Control-Allow-Origin": "*"
          }
        }
    ]
    
}